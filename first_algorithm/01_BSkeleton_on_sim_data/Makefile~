N=31600


EXEC_PATH=/home/filipo/master_thesis/ngl-beta/build/binsrc/

# Skeleton parameters
METHOD=BSkeleton
BETA=1.7

FILE_NGL=N_$(N)
FILE_SKEL=BS_N_$(N)


$(FILE_SKEL): $(FILE_NGL)
	$(EXEC_PATH)./getNeighborGraph -i $(FILE_NGL) -d 3 -b $(BETA) -m $(METHOD) > $(FILE_SKEL)


clean:
	rm $(FILE_SKEL)
	rm -f *~
